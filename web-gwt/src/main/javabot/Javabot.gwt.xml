    <!-- Specify the app entry point class.                   -->
    <entry-point class="javabot.client.Javabot"/>


    <!-- Specify the app servlets.                   -->
    <servlet path="/javabot.Javabot/JavabotService" class="javabot.server.JavabotServiceImpl"/>

</module>