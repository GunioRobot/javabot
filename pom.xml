    <repositories> <repository> <id>java.net</id>
    <url>http://download.java.net/maven/2/</url> </repository>

    <repository> <id>antwerkz</id> <url>http://antwerkz.com/</url>
    </repository> <repository> <id>dev.java.net</id>
    <url>https://maven-repository.dev.java.net/nonav/repository/</url>
    <layout>legacy</layout> </repository> </repositories>
  -->
  <properties>
    <wicket.version>1.3.5</wicket.version>
  </properties>
</project>